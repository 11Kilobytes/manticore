#include "types.def"

extern float sqrtf (float) __attribute__((pure));

define @float-sqrt (x : ml_float / exh : exh) : ml_float =
    let y = ccall sqrtf (unwrap(x))
    let res : ml_float = wrap(y)
      return (res)
;
