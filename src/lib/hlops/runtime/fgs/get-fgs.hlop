#include "fgs.def"
#include "assert.def"

define @get-fgs (vp : vproc / exh : exh) : fgs =
  let fgs : fgs = vpload (CURRENT_FG, vp)
  do assert(NotEqual (fgs, NIL))
  return (fgs)
;
