 *
 * Allocate an empty locked queue.
 *
 * We represent queues as a linked structure.
 *     hd -> elt1 -> elt2 -> elt3
 *                             ^
 *                             |
 *                             tl
 *)

#include "locked-queue.def"

define @locked-queue-new ( / exh : exh) : locked_queue =
  let lockedQ : locked_queue = galloc (FALSE, LOCKED_QUEUE_EMPTY, LOCKED_QUEUE_EMPTY, LOCKED_QUEUE_EMPTY, LOCKED_QUEUE_EMPTY)
  return (lockedQ)
;
