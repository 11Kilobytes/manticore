#include "types.def"
#include "ropes.def"

define inline @plen (r : rope / exh : exh) : int =
  case r
    of Leaf (len:int, data:list) => return (len)
     | Cat (len:int, depth:int, r1:rope, r2:rope) => return (len)
  end
;

