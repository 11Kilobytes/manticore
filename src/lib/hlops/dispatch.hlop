 *
 * Switch to the next thread from the host-vproc's ready queue.
 *)

#include "types.def"

define @dequeue (vp : vproc) : (tid, fiber);
define @switch-to (vp : vproc, tid : tid, fiber : fiber) noreturn;

define inline @dispatch (vp : vproc) noreturn =
    let (tid : tid, fiber : fiber) = @dequeue (vp)
    @switch-to (vp, tid, fiber)
;
