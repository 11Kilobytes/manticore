#include "types.def"

extern void *M_NewImage (int, int);

define inline @image-new (arg : [ml_int, ml_int] / exh : exh) : any =
  let img : any = ccall M_NewImage (unwrap(#0(arg)), unwrap(#1(arg)))
    return (img)
;
