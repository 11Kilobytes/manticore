#include "types.def"

extern void M_UpdateImage (void *img, int row, int col, float r, float g, float b);

define inline image-update (arg : [any, int, int, float, float, float] / exh : exh) : unit =
  let () = M_UpdateImage (#0(arg), #1(arg), #2(arg), #3(arg), #4(arg), #5(arg))
    return (UNIT)
;
