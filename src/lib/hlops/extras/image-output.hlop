#include "types.def"

extern void void M_OutputImage (void *img, void *file);

define inline image-output (arg : [any, ml_string] / exh : exh) : unit =
  let file : any = @string-data(#1(arg))
  let () = M_OutputImage (#0(arg), file)
    return (UNIT)
;
