#include "types.def"

extern void *M_StringConcat2 (void *, void *) __attribute__((pure,alloc));

define inline @string-concat2 (arg : [ml_string, ml_string] / _ : exh) : ml_string =
    let res : ml_string = ccall M_StringConcat2 (#0(arg), #1(arg))
      return (res)
;
