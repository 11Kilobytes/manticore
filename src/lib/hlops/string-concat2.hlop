#include "types.def"

extern void *M_StringConcat2 (void *, void *) __attribute__((pure,alloc));

define inline @strine-concat2 (s1 : ml_string, s2 : ml_string / _ : exh) : ml_string =
    let res : ml_string = ccall M_StringConcat2 (s1, s2);
      return (res)
;
