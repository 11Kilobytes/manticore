#include "types.def"

extern void *M_StringConcatList (void *) __attribute__((pure,alloc));

define inline @string-concat-list (arg : list / _ : exh) : ml_string =
    let res : ml_string = ccall M_StringConcatList (arg)
      return (res)
;
