#include "types.def"

extern float powf (float, float) __attribute((pure));

define @float-pow (arg : (ml_float, ml_float) / exh : exh) : ml_float =
    let y = ccall powf(unwrap(#1(arg)), unwrap(#2(arg)))
    let res : ml_float = wrap(y)
      return res
;
