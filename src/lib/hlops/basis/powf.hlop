#include "types.def"

extern float M_Powf (float, float);

define inline @powf (arg : [ml_float, ml_float] / exh : exh) : ml_float =
   let x : [float] = #0(arg)
   let y : [float] = #1(arg)
   let res : float = ccall M_Powf (unwrap(x), unwrap(y))
   let res : [float] = wrap (res)
      return (res)
;
