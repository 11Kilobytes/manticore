#include "types.def"

extern double M_Powd (double, double) __attribute((pure));

define @double-pow (arg : (ml_double, ml_double) / exh : exh) : ml_double =
    let x : [double] = #0(arg)
    let y : [double] = #1(arg)
    let res : double = ccall M_Powd (unwrap (x), unwrap (y))
    let res : ml_double = wrap(res)
      return res
;
